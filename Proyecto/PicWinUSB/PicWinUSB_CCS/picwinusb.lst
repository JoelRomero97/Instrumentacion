....................          'C',0, 
....................          '-',0, 
....................          'U',0, 
....................          'S',0, 
....................          'B',0, 
....................          '-',0, 
....................          'I',0, 
.................... 		 'N',0, 
.................... 		 'T',0, 
.................... 		 'R',0, 
.................... 		 'U',0, 
.................... 		 'M',0, 
.................... 		 'E',0, 
.................... 		 'N',0, 
.................... 		 'T',0, 
